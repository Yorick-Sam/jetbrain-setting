<keymap version="1" name="Default copy" parent="$default">
  <action id="ActivateApplicationServersToolWindow">
    <keyboard-shortcut first-keystroke="ctrl alt 2" />
  </action>
  <action id="ActivateCheckStyleToolWindow">
    <keyboard-shortcut first-keystroke="ctrl alt 1" />
  </action>
  <action id="Annotate">
    <keyboard-shortcut first-keystroke="shift ctrl alt z" />
  </action>
  <action id="NewClass">
    <keyboard-shortcut first-keystroke="shift ctrl alt space" />
  </action>
  <action id="NewDir">
    <keyboard-shortcut first-keystroke="shift ctrl alt period" />
  </action>
  <action id="SonarLint.AnalyzeAllFiles">
    <keyboard-shortcut first-keystroke="shift ctrl alt q" />
  </action>
  <action id="com.ivanovych666.intellij.plugin.jsonsorter.NaturalCiSort">
    <keyboard-shortcut first-keystroke="ctrl alt back_quote" />
  </action>
  <action id="de.netnexus.CamelCasePlugin.ToggleCamelCase">
    <keyboard-shortcut first-keystroke="shift alt u" />
    <keyboard-shortcut first-keystroke="ctrl alt q" />
  </action>
</keymap>